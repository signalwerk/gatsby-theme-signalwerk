{
  "name": "gatsby-theme-signalwerk",
  "description": "Gatsby Theme from signalwerk",
  "version": "0.0.1",
  "author": "Stefan Huber <sh@signalwerk.ch>",
  "bugs": {
    "url": "https://github.com/signalwerk/gatsby-theme-signalwerk/issues"
  },
  "dependencies": {
    "gatsby": "^2.0.19",
    "gatsby-plugin-feed": "^2.0.8",
